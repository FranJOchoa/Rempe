					buttons="{KeyValue>/MOV_FSTYDA}"
					id="rMOV_FSTYDA"
					columns="2"
					width="100%"
					class="sapUiMediumMarginBottom"
					enabled="{
						parts: ['CAD_AE>/Dokst', 'CAD_AE>/content/MOV_FDISFL/Value'],
						formatter: '.formatter.isEditable'
					}"
					select="DisChargeUrgeRadioButtom"
				>
