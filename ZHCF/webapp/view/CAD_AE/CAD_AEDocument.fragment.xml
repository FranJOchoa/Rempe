				mode="{= ${
						path: 'CAD_AE>/Dokst',
						formatter: '.formatter.isEditable'
					}?'Delete':'None'}"
				delete="onDeleteServicesTablePressed($event, 'CAD_AE')"
			>
				<dragDropConfig>
					<dnd:DropInfo drop="onDropServices($event, 'CAD_AE')"/>
				</dragDropConfig>
				<columns>
					<Column>
						<Text text="{i18n>Date}"/>
					</Column>
					<Column>
						<Text text="{i18n>Test}"/>
					</Column>
					<Column>
						<Text text="{i18n>Doctor}"/>
					</Column>
					<Column>
						<Text text="{i18n>CADMAM_PRU_RES}"/>
					</Column>
				</columns>
				<ColumnListItem>
