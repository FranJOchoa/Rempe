					id="rMOV_FTRADC"
					columns="3"
					width="100%"
					class="sapUiMediumMarginBottom"
					enabled="{
						parts: ['CAD_AE>/Dokst', 'CAD_AE>/content/MOV_FDISFL/Value', 'CAD_AE>/content/MOV_FTRADI/Value'],
						formatter: '.formatter.isEditableAndChecked'
					}"
					select="DisChargeUrgeRadioButtom"
				>
